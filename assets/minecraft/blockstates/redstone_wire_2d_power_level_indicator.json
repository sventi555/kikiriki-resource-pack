{
    "multipart": [
        {   "when": { "OR": [
                {"north": "none", "east": "none", "south": "none", "west": "none"},
                {"north": "side|up", "east": "side|up" },
                {"east": "side|up", "south": "side|up" },
                {"south": "side|up", "west": "side|up"},
                {"west": "side|up", "north": "side|up"}
            ]},
            "apply": { "model": "block/redstone_2d/redstone_dust_dot" }
        },
        {   "when": { "OR": [
                { "north": "side|up" },
                { "north": "none", "east": "none", "south": "side|up", "west": "none" }
            ]},
            "apply": { "model": "block/redstone_2d/redstone_dust_side0" }
        },
        {   "when": { "OR": [
                { "south": "side|up" },
                { "north": "side|up", "east": "none", "south": "none", "west": "none" }
            ]},
            "apply": { "model": "block/redstone_2d/redstone_dust_side_alt0" }
        },
        {   "when": { "OR": [
                { "east": "side|up" },
                { "north": "none", "east": "none", "south": "none", "west": "side|up" }
            ]},
            "apply": { "model": "block/redstone_2d/redstone_dust_side_alt1", "y": 270 }
        },
        {   "when": { "OR": [
                { "west": "side|up" },
                { "north": "none", "east": "side|up", "south": "none", "west": "none" }
            ]},
            "apply": { "model": "block/redstone_2d/redstone_dust_side1", "y": 270 }
        },
        {   "when": { "north": "up" },
            "apply": { "model": "block/redstone_2d/redstone_dust_up" }
        },
        {   "when": { "east": "up" },
            "apply": { "model": "block/redstone_2d/redstone_dust_up", "y": 90 }
        },
        {   "when": { "south": "up" },
            "apply": { "model": "block/redstone_2d/redstone_dust_up", "y": 180 }
        },
        {   "when": { "west": "up" },
            "apply": { "model": "block/redstone_2d/redstone_dust_up", "y": 270 }
        },        
        {    "when": { "power": 0 },
            "apply": { "model": "block/redstone_power_0_2d"}
        }, 
        {    "when": { "power": 1 },
            "apply": { "model": "block/redstone_power_1_2d"}
        },
        {    "when": { "power": 2 },
            "apply": { "model": "block/redstone_power_2_2d"}
        },
        {    "when": { "power": 3 },
            "apply": { "model": "block/redstone_power_3_2d"}
        },
        {    "when": { "power": 4 },
            "apply": { "model": "block/redstone_power_4_2d"}
        },
        {    "when": { "power": 5 },
            "apply": { "model": "block/redstone_power_5_2d"}
        },
        {    "when": { "power": 6 },
            "apply": { "model": "block/redstone_power_6_2d"}
        },
        {   "when": { "power": 7 },
            "apply": { "model": "block/redstone_power_7_2d"}
        },
        {    "when": { "power": 8 },
            "apply": { "model": "block/redstone_power_8_2d"}
        },
        {    "when": { "power": 9 },
            "apply": { "model": "block/redstone_power_9_2d"}
        },
        {    "when": { "power": 10 },
            "apply": { "model": "block/redstone_power_10_2d"}
        },
        {    "when": { "power": 11 },
            "apply": { "model": "block/redstone_power_11_2d"}
        },
        {    "when": { "power": 12 },
            "apply": { "model": "block/redstone_power_12_2d"}
        },
        {    "when": { "power": 13 },
            "apply": { "model": "block/redstone_power_13_2d"}
        },
        {    "when": { "power": 14 },
            "apply": { "model": "block/redstone_power_14_2d"}
        },
        {    "when": { "power": 15 },
            "apply": { "model": "block/redstone_power_15_2d"}
        }
    ]
}
